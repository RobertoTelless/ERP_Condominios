                                    @if (cond.SETP_IN_ATIVO == 1)
                                    {
                @Html.TableLinkDetails(cond.SETP_CD_ID, "ExcluirTabelaServico", "fa-trash-o", "Desativar") }
            else
            {
@Html.TableLinkDetails(cond.SETP_CD_ID, "ReativarTabelaServico", "fa-refresh", "Reativar")}
                                </td>
                            </tr>}
                                        </tbody>
                                    </table>}
                                            </div>
                                        </div>

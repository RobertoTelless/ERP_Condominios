        <h5 class="modal-title">Editar custo</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            @Html.HiddenFor(x => x.SERV_CD_ID)
                            <input id="prtpCd" type="hidden" name="id" />
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                Filial
                                @Html.DropDownListFor(m => m.FILI_CD_ID, (SelectList)ViewBag.Filiais, "Selecionar", new { @class = "form-control", autofocus = true, id = "filialModal" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Custo
                                @Html.TextBox("SETP_VL_CUSTO", new { @class = "form-control", autofocus = true, id = "custoModal", onfocusout = "CalculaPrecoEdicao()" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Markup
                                @Html.TextBox("SETP_NR_MARKUP", new { @class = "form-control", autofocus = true, id = "markupModal", onfocusout = "CalculaPrecoEdicao()" })
                            </div>
                            <div class="col-lg-2 col-md-2 col-xs-12">
                                Preço (R$)
                                @Html.TextBox("SETP_VL_PRECO", new { @class = "form-control", autofocus = true, id = "precoModal" })
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                Preço Promoção (R$)
                                @Html.TextBox("SETP_VL_PRECO_PROMOCAO", new { @class = "form-control", autofocus = true, id = "precoPromoModal" })
                            </div>
                        </div>
                    </div>
                                    <div class="modal-footer">
                                        @Html.ButtonActionSubmit(text: "Confirma edição", htmlAttributes: new { id = "basictoast" })
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                                    </div>}
            </div>
        </div>

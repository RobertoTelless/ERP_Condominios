            @Html.DropDownListFor(m => m.NBSE_CD_ID, (SelectList)ViewBag.Nomes, "Selecionar", new { @class = "form-control", autofocus = true })
            @Html.ValidationMessageFor(m => m.NBSE_CD_ID, "", new { @style = "color:red" })

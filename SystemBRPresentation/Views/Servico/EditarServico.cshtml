            @Html.TextBoxFor(m => m.SERV_VL_PRECO, new { @class = "form-control", autofocus = true })
            @Html.ValidationMessageFor(m => m.SERV_VL_PRECO, "", new { @style = "color:red" })
        </div>
    </div>
    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">

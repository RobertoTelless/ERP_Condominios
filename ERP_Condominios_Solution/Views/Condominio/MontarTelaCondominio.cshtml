@using Presentation.Extensions
@model ERP_Condominios_Solution.ViewModels.AssinanteViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="page-title">
        <h3 class="title">Condomínio</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li class="active">
                <strong>Condomínio - Informações Gerais</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Condomínio - Informações Gerais</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>

                <div class="ibox-content">
                    @*<div class="form-group">
                        <br />
                        @Html.ButtonLinkIndex(text: "Voltar", action: "Voltar", controller: "Condominio")
                    </div>*@
                    @*@Html.AntiForgeryToken()*@
                    @if (!ViewData.ModelState.IsValid)
                    {
                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert">Fechar</button>
                            @Html.ValidationSummary(false, "", new { @class = "text-danger" })
                        </div>
                    }
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Foto & Anexos</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Unidades</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-4">Vagas</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-5">Observações</a></li>
                        </ul>
                        <div class="tab-content">

                            @*Dados Gerais*@
                            <div id="tab-1" class="tab-pane active">
                                <div class="panel-body">
                                    @using (Html.BeginForm("MontarTelaCondominio", "Condominio", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row" style="width: 100%;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                    @Html.ButtonActionSubmit(text: "Confirma edição de Dados do Condomínio", htmlAttributes: new { id = "basictoast" })
                                                    @Html.ButtonLinkIndex(text: "Voltar", action: "Voltar", controller: "Condominio")
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row" style="width: 100%;">
                                            <div class="col-lg-2 col-md-2 col-xs-12">
                                                <div style="text-align: center; margin-top: 25px; line-height: 10px;">
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    @Html.HiddenFor(m => m.ASSI_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.ASSI_IN_STATUS)
                                                    @Html.HiddenFor(m => m.ASSI_IN_TIPO)
                                                    @if (!String.IsNullOrEmpty(Model.ASSI_AQ_FOTO))
                                                    {
                                                        <img src="@Url.Content(Model.ASSI_AQ_FOTO)" style="width: auto; height: auto; max-height: 110px; max-width: 90px;" data-toggle="modal" data-target="#fotoModal" onclick="CriaModal('@Model.ASSI_NM_NOME', '@Url.Content(Model.ASSI_AQ_FOTO)')">
                                                    }
                                                    else
                                                    {
                                                        <img src="~/Images/gallery/10s.jpg" style="width: auto; height: auto; max-height: 110px; max-width: 90px;">
                                                    }
                                                    <br />
                                                    <br />
                                                    <br />
                                                    <br />
                                                    <br />
                                                </div>
                                            </div>
                                            <div class="col-md-10">
                                                <div class="row">
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                        <div class="col-lg-4 col-md-4 col-xs-12">
                                                            Nome*
                                                            @Html.TextBoxFor(m => m.ASSI_NM_NOME, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.ASSI_NM_NOME, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-3 col-md-3 col-xs-12">
                                                            Tipo de Pessoa*
                                                            @Html.DropDownListFor(m => m.TIPE_CD_ID, (SelectList)ViewBag.Tipos, "Selecionar", new { @class = "form-control", autofocus = true, @id = "pessoa", onchange = "ValidaDiv($(this).val())" })
                                                            @Html.ValidationMessageFor(m => m.TIPE_CD_ID, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                                            Tipo de Condomínio*
                                                            @Html.DropDownListFor(m => m.TICO_CD_ID, (SelectList)ViewBag.TC, "Selecionar", new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.TICO_CD_ID, "", new { @style = "color:red" })
                                                        </div>
                                                        <div class=" col-lg-2 col-md-2 col-xs-12">
                                                            Data Início
                                                            @Html.TextBoxFor(m => m.ASSI_DT_INICIO, "{0:dd/MM/yyyy}", new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                            @Html.ValidationMessageFor(m => m.ASSI_DT_INICIO, "", new { @style = "color:red" })
                                                        </div>                                                
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                        <div class="col-lg-4 col-md-4 col-xs-12">
                                                            Email*
                                                            @Html.TextBoxFor(m => m.ASSI_NM_EMAIL, new { @class = "form-control", autofocus = true })
                                                            @Html.ValidationMessageFor(m => m.ASSI_NM_EMAIL, "", new { @style = "color:red" })
                                                        </div>                                                 
                                                    </div>
                                                </div>
                                                <br />
                                            </div>
                                        </div>
                                        <div class="row" style="width: 100%;">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="PF">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    CPF
                                                    @Html.TextBoxFor(m => m.ASSI_NR_CPF, new { @class = "form-control", @id = "cpf", autofocus = true })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NR_CPF, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;" id="PJ">
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    CNPJ
                                                    @Html.TextBoxFor(m => m.ASSI_NR_CNPJ, new { @class = "form-control", @id = "cnpj" })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NR_CNPJ, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-6 col-md-6 col-xs-12">
                                                    Razão Social
                                                    @Html.TextBoxFor(m => m.ASSI_NM_RAZAO_SOCIAL, new { @class = "form-control", autofocus = true, id = "rs" })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NM_RAZAO_SOCIAL, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    <h4>Endereço</h4>
                                                    <br />
                                                    CEP
                                                    <div class="input-group">
                                                        @Html.TextBoxFor(m => m.ASSI_NR_CEP, new { @class = "form-control", @id = "cep", autofocus = true })
                                                        @Html.ValidationMessageFor(m => m.ASSI_NR_CEP, "", new { @style = "color:red" })
                                                        <span class="input-group-btn">
                                                            <span class="btn btn-warning btn-sm" id="btnBuscaCep" onclick="PesquisaCep($('#cep').val(), $(this), 1)">Buscar CEP</span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Endereço*
                                                    @Html.TextBoxFor(m => m.ASSI_NM_ENDERECO, new { @class = "form-control", autofocus = true, @id = "endereco" })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NM_ENDERECO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Número*
                                                    @Html.TextBoxFor(m => m.ASSI_NR_NUMERO, new { @class = "form-control", autofocus = true, @id = "nmr" })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NR_NUMERO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    Bairro*
                                                    @Html.TextBoxFor(m => m.ASSI_NM_BAIRRO, new { @class = "form-control", autofocus = true, @id = "bairro" })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NM_BAIRRO, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-3 col-md-3 col-xs-12">
                                                    Cidade*
                                                    @Html.TextBoxFor(m => m.ASSI_NM_CIDADE, new { @class = "form-control", autofocus = true, @id = "cidade" })
                                                    @Html.ValidationMessageFor(m => m.ASSI_NM_CIDADE, "", new { @style = "color:red" })
                                                </div>
                                                <div class="col-lg-2 col-md-2 col-xs-12">
                                                    UF*
                                                    @Html.DropDownListFor(m => m.UF_CD_ID, (SelectList)ViewBag.UF, "Selecionar", new { @class = "form-control", autofocus = true, @id = "uf" })
                                                    @Html.ValidationMessageFor(m => m.UF_CD_ID, "", new { @style = "color:red" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            @if (Model.TIPE_CD_ID == 2)
                                            {
                                                <div class="form-group">
                                                    <div class="col-lg-12 col-md-12 col-xs-12">
                                                        <hr />
                                                        <h4>Quadro Diretor</h4>
                                                        <br />
                                                        <table style="width: 100%;" class="table table-striped table-bordered table-hover dataTables-example" id="tblQs">
                                                            <thead style="background-color:lightblue">
                                                                <tr>
                                                                    <th>Cargo</th>
                                                                    <th>Nome</th>
                                                                    <th>Unidade</th>
                                                                    <th>Telefone</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in Model.CORPO_DIRETIVO)
                                                                {
                                                                    <tr>
                                                                        <td>@cond.FUNCAO_CORPO_DIRETIVO.FUCO_NM_NOME</td>
                                                                        <td>@cond.USUARIO.USUA_NM_NOME</td>
                                                                        <td>@cond.USUARIO.UNIDADE.UNID_NM_EXIBE</td>
                                                                        <td>@cond.USUARIO.USUA_NR_CELULAR</td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                        <div class="form-group">
                                            @Html.ButtonActionSubmit(text: "Confirma edição de Dados do Condomínio", htmlAttributes: new { id = "basictoast", style = "margin-bottom: -35px;" })
                                        </div>
                                    }
                                </div>
                            </div>
                            
                            @*Imagens - Foto e Anexos*@
                            <div id="tab-2" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-10 col-md-10 col-xs-12">
                                            <div class="form-group">
                                                <h4>Foto</h4>
                                                @using (Html.BeginForm("UploadFotoCondominio", "Condominio", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { enctype = "multipart/form-data", @id = "pwd-container3" }))
                                                {
                                                    <div class="form-group">
                                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                                            <input style="float: left; margin-right: 5px;" type="file" accept=".jpg, .png, .jpeg, .gif|images/*" name="file" id="file" class="btn btn-sm btn-warning" />
                                                            <input style="float: left;" type="submit" value="Incluir Foto do Condomínio" class="btn btn-sm btn-success" />
                                                        </div>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-xs-12">
                                            <div class="form-group">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <br />
                                                    @if (!String.IsNullOrEmpty(Model.ASSI_AQ_FOTO))
                                                    {
                                                        <img src="@Url.Content(Model.ASSI_AQ_FOTO)" style="width: auto; height: auto; max-height: 110px; max-width: 90px;" data-toggle="modal" data-target="#fotoModal" onclick="CriaModal('@Model.ASSI_NM_NOME', '@Url.Content(Model.ASSI_AQ_FOTO)')">
                                                    }
                                                    else
                                                    {
                                                        <img src="~/Images/gallery/10s.jpg" style="width: auto; height: auto; max-height: 110px; max-width: 90px;">
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <hr />
                                        <div class="col-lg-12 col-md-12 col-xs-12">
                                            <h4>Arquivos Anexos</h4>
                                            <br />
                                            <div class="form-group">
                                                @using (Html.BeginForm("UploadFileCondominio", "Condominio", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { enctype = "multipart/form-data", @id = "pwd-container2" }))
                                                {
                                                    <div class="ibox-content">
                                                        @if (Model.ASSINANTE_ANEXO.Count > 0)
                                                        {
                                                            <table class="table table-striped table-bordered table-hover dataTables-example">
                                                                <thead style="background-color:lightsteelblue">
                                                                    <tr>
                                                                        <th>Título</th>
                                                                        <th>Data</th>
                                                                        <th>Tipo</th>
                                                                        <th class="td-one-action">Ações</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    @foreach (var cond in (Model.ASSINANTE_ANEXO))
                                                                    {
                                                                        <tr>
                                                                            <td>@cond.ASAN_NM_TITULO</td>
                                                                            <td>@cond.ASAN_DT_ANEXO.ToShortDateString()</td>
                                                                            @if (cond.ASAN_IN_TIPO == 1)
                                                                            {
                                                                                <td>Imagem</td>
                                                                            }
                                                                            else if (cond.ASAN_IN_TIPO == 2)
                                                                            {
                                                                                <td>Vídeo</td>
                                                                            }
                                                                            else
                                                                            {
                                                                                <td>Arquivo</td>
                                                                            }
                                                                            <td class="td-one-action">
                                                                                @if (cond.ASAN_IN_TIPO == 1 || cond.ASAN_IN_TIPO == 2)
                                                                                {
                                                                                    @Html.TableLinkDetails(cond.ASAN_CD_ID, "VerAnexoCondominio", "fa-camera", "Visualizar")
                                                                                }
                                                                                @if (cond.ASAN_IN_TIPO == 3)
                                                                                {
                                                                                    <a title="Visualizar" class="fa fa-camera fa-lg" href="~/PdfViewer/web/viewer.html?file=@(cond.ASAN_AQ_ARQUIVO.Replace("~", ""))&ReturnUrl=@Request.Url.AbsolutePath" target="_blank" ></a>
                                                                                }
                                                                                @Html.TableLinkDetails(cond.ASAN_CD_ID, "DownloadCondominio", "fa-download", "Download")
                                                                            </td>
                                                                        </tr>
                                                                    }
                                                                </tbody>
                                                            </table>
                                                        }
                                                        <div class="form-group">
                                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                                <input style="float: left; margin-right: 5px;" type="file" name="file" id="file" class="btn btn-sm btn-warning" />
                                                                <input style="float: left; margin-right: 5px;" type="submit" value="Incluir Anexo" class="btn btn-sm btn-success" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Unidades*@
                            <div id="tab-3" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <span class="label label-primary pull-right">Hoje</span>
                                                    <h5><a href="@Url.Action("MontarTelaUnidade", "Unidade")" class="pull-left btn-link"><u>Unidades</u></a></h5>
                                                </div>
                                                <div class="ibox-content">
                                                    @if (Model.UNIDADE.Count > 0)
                                                    {
                                                        <table class="table table-hover">
                                                            <thead>
                                                                <tr>
                                                                    <th>Tipo</th>
                                                                    <th>Número</th>
                                                                    <th>Moradores</th>
                                                                    <th>Veículos</th>
                                                                    <th>Alugada?</th>
                                                                    <th class="td-one-action">Ações</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (Model.UNIDADE.OrderBy(a => a.UNID_NR_NUMERO)))
                                                                {
                                                                    <tr>
                                                                        <td>@cond.TIPO_UNIDADE.TIUN_NM_NOME</td>
                                                                        <td>@cond.UNID_NM_EXIBE</td>
                                                                        <td>@cond.USUARIO.Count</td>
                                                                        <td>@cond.VEICULO.Count</td>
                                                                        @if (cond.UNID_IN_ALUGADA == 1)
                                                                        {
                                                                            <td>Sim</td>
                                                                        }
                                                                        else
                                                                        {
                                                                            <td>Não</td>
                                                                        }
                                                                        <td class="td-one-action">
                                                                            @Html.TableLinkDetails(cond.UNID_CD_ID, "VerUnidade", "fa-search", "Ver Unidade")
                                                                        </td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Vagas*@
                            <div id="tab-4" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <span class="label label-primary pull-right">Hoje</span>
                                                    <h5><a href="@Url.Action("MontarTelaVaga", "Vaga")" class="pull-left btn-link"><u>Vagas</u></a></h5>
                                                </div>
                                                <div class="ibox-content">
                                                    @if (Model.VAGA.Count > 0)
                                                    {
                                                        <table class="table table-hover">
                                                            <thead>
                                                                <tr>
                                                                    <th>Tipo</th>
                                                                    <th>Andar</th>
                                                                    <th>Número</th>
                                                                    <th>Unidade</th>
                                                                    <th>Data de Atribuição</th>
                                                                    <th class="td-one-action">Ações</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (Model.VAGA.OrderBy(a => a.VAGA_NR_NUMERO)))
                                                                {
                                                                    <tr>
                                                                        <td>@cond.TIPO_VAGA.TIVA_NM_NOME</td>
                                                                        <td>@cond.VAGA_NR_ANDAR</td>
                                                                        <td>@cond.VAGA_NR_NUMERO</td>
                                                                        @if (cond.UNIDADE != null)
                                                                        {
                                                                            <td>@cond.UNIDADE.UNID_NM_EXIBE</td>
                                                                        }
                                                                        else
                                                                        {
                                                                            <td>Condomínio</td>
                                                                        }
                                                                        @if (cond.VAGA_DT_ATRIBUICAO != null)
                                                                        {
                                                                            <td>@cond.VAGA_DT_ATRIBUICAO.Value.ToShortDateString()</td>
                                                                        }
                                                                        else
                                                                        {
                                                                            <td>-</td>
                                                                        }
                                                                        <td class="td-one-action">
                                                                            @Html.TableLinkDetails(cond.VAGA_CD_ID, "VerVaga", "fa-search", "Ver Vaga")
                                                                        </td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Observações*@
                            <div id="tab-5" class="tab-pane">
                                <div class="panel-body">
                                    @using (Html.BeginForm("MontarTelaCondominio", "Condominio", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row">
                                            <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                                <div class="col-lg-12 col-md-12 col-xs-12">
                                                    @Html.HiddenFor(m => m.ASSI_CD_ID)
                                                    @Html.HiddenFor(m => m.ASSI_AQ_FOTO)
                                                    @Html.HiddenFor(m => m.ASSI_DT_INICIO)
                                                    @Html.HiddenFor(m => m.ASSI_IN_ATIVO)
                                                    @Html.HiddenFor(m => m.ASSI_IN_STATUS)
                                                    @Html.HiddenFor(m => m.ASSI_IN_TIPO)
                                                    @Html.HiddenFor(m => m.ASSI_NM_BAIRRO)
                                                    @Html.HiddenFor(m => m.ASSI_NM_CIDADE)
                                                    @Html.HiddenFor(m => m.ASSI_NM_EMAIL)
                                                    @Html.HiddenFor(m => m.ASSI_NM_ENDERECO)
                                                    @Html.HiddenFor(m => m.ASSI_NM_NOME)
                                                    @Html.HiddenFor(m => m.ASSI_NM_RAZAO_SOCIAL)
                                                    @Html.HiddenFor(m => m.ASSI_NR_CEP)
                                                    @Html.HiddenFor(m => m.ASSI_NR_CNPJ)
                                                    @Html.HiddenFor(m => m.ASSI_NR_CPF)
                                                    @Html.HiddenFor(m => m.ASSI_NR_NUMERO)
                                                    @Html.HiddenFor(m => m.TICO_CD_ID)
                                                    @Html.HiddenFor(m => m.TIPE_CD_ID)

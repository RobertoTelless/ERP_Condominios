@using Presentation.Extensions
@model ERP_Condominios_Solution.ViewModels.UsuarioViewModel
@{
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">

    @using (Html.BeginForm("EnviarSmsMorador", "Morador", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal" }))
    { 
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Enviar SMS</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                      @Html.Hidden("id", null, new { id = "idUsua" })
                      Mensagem
                      @Html.TextBox("mensagem", null, new { @style = "height: 200px;", @class = "form-control" })
                  </div>
                  <div class="modal-footer">
                      @Html.ButtonActionSubmit("Enviar")
                    <button type="button" class="btn btn-sm btn-warning" data-dismiss="modal">Cancelar</button>
                  </div>
                </div>
              </div>
            </div>
    }

    <div class="page-title">
        <h3 class="title">Morador</h3>
        <br />
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("CarregarBase", "BaseAdmin")">Dashboard</a>
            </li>
            <li class="active">
                <strong>Morador - Consulta</strong>
            </li>
        </ol>
        <br />
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="col-lg-12 col-md-12 col-xs-12 ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Morador - Consulta</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-wrench"></i>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#">Configuração 1</a>
                            </li>
                            <li>
                                <a href="#">Configuração 2</a>
                            </li>
                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>

                <div class="ibox-content">
                    <div class="form-group">
                        <br />
                        <span class="btn btn-sm btn-warning" onclick="MudaIdForn('@Model.USUA_CD_ID')" data-toggle="modal" data-target="#exampleModal">Enviar SMS</span>
                        @Html.ButtonLinkIndex(text: "Voltar", action: "Voltar", controller: "Morador")
                    </div>
                    <div class="tabs-container">
                        <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#tab-1">Dados Gerais</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-2">Moradores da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-3">Veículos da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-4">Visitantes da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-5">Encomendas da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-6">Reservas da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-7">Mudanças da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-8">Autorizações da Unidade</a></li>
                                <li class=""><a data-toggle="tab" href="#tab-9">Ocorrências</a></li>
                        </ul>
                        <div class="tab-content">

                            @*Dados Gerais*@
                            <div id="tab-1" class="tab-pane active">
                                <div class="panel-body">
                                    @using (Html.BeginForm("MontarTelaMorador", "Morador", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", @id = "pwd-container1" }))
                                    {
                                        <div class="row" style="width: 100%;">
                                            <div class="col-lg-1 col-md-1 col-xs-12">
                                                <div style="text-align: center; margin-top: 25px; line-height: 10px;">
                                                    @if (!String.IsNullOrEmpty(Model.USUA_AQ_FOTO))
                                                    {
                                                        <img src="@Url.Content(Model.USUA_AQ_FOTO)" style="width: auto; height: auto; max-height: 110px; max-width: 90px;">
                                                    }
                                                    else
                                                    {
                                                        <img src="~/Images/gallery/10s.jpg" style="width: auto; height: auto; max-height: 110px; max-width: 90px;">
                                                    }
                                                    <br />
                                                    <br />
                                                    <br />
                                                    <br />
                                                    <br />
                                                </div>
                                            </div>
                                            <div class="col-md-11">
                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-xs-12">
                                                        Nome
                                                        @Html.TextBoxFor(m => m.USUA_NM_NOME, new { @class = "form-control", autofocus = true, @readonly = "readonly"})
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Telefone
                                                        @Html.TextBoxFor(m => m.USUA_NR_TELEFONE, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                    </div>
                                                    <div class="col-lg-2 col-md-2 col-xs-12">
                                                        Celular
                                                        @Html.TextBoxFor(m => m.USUA_NR_CELULAR, new { @class = "form-control", autofocus = true, @readonly = "readonly" })
                                                    </div>
                                                    <div class="col-lg-4 col-md-4 col-xs-12">
                                                        E-Mail
                                                        @Html.TextBoxFor(m => m.USUA_NM_EMAIL, new { @class = "form-control", autofocus = true, @readonly = "readonly"})
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>

                            @*Moradores da Unidade*@
                            <div id="tab-2" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="ibox float-e-margins">
                                                <div class="ibox-title">
                                                    <span class="label label-primary pull-right">Hoje</span>
                                                    <h5>Moradores da Unidade</h5>
                                                </div>
                                                <div class="ibox-content">
                                                    @if ((ViewBag.Moradores as List<EntitiesServices.Model.USUARIO>).Count > 0)                                        
                                                    {
                                                        <table class="table table-striped table-bordered table-hover dataTables-example">
                                                            <thead style ="background-color:lightsteelblue">
                                                                <tr>
                                                                    <th>Nome</th>   
                                                                    <th>Unidade</th>   
                                                                    <th>Telefone</th>
                                                                    <th>Celular</th>
                                                                    <th>E-Mail</th>   
                                                                    <th></th>   
                                                                    <th class="td-one-action">Ações</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var cond in (ViewBag.Moradores as List<EntitiesServices.Model.USUARIO>).OrderBy(p => p.USUA_NM_NOME))
                                                                {
                                                                    <tr>
                                                                        <td>@cond.USUA_NM_NOME</td>
                                                                        <td>@cond.UNIDADE.UNID_NM_EXIBE</td>
                                                                        <td>@cond.USUA_NR_TELEFONE</td>
                                                                        <td>@cond.USUA_NR_CELULAR</td>
                                                                        <td>@cond.USUA_NM_EMAIL</td>
                                                                        @if (!String.IsNullOrEmpty(cond.USUA_AQ_FOTO))
                                                                        {
                                                                            <td><img src="@Url.Content(cond.USUA_AQ_FOTO)" style="width: auto; height: auto; max-height: 55px; max-width: 45px;" data-toggle="modal" data-target="#fotoModal" onclick="CriaModal('@cond.USUA_NM_NOME', '@Url.Content(cond.USUA_AQ_FOTO)')"></td>
                                                                        }
                                                                        else
                                                                        {
                                                                            <td><img src="~/Images/gallery/a7.jpg" style="width: auto; height: auto; max-height: 55px; max-width: 45px;" data-toggle="modal" data-target="#fotoModal" onclick="CriaModal('@cond.USUA_NM_NOME', '/Images/gallery/a7.jpg')"></td>
                                                                        }
                                                                        <td class="td-one-action">
                                                                            @Html.TableLinkDetails(cond.USUA_CD_ID, "VerMorador","fa-search", "Consultar")
                                                                        </td>
                                                                    </tr>
                                                                }
                                                            </tbody>
                                                        </table>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Veiculos*@
                            <div id="tab-3" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Veículos da Unidade</h4>
                                                <br />
                                                @if (Model.UNIDADE.VEICULO.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Placa</th>
                                                                <th>Marca</th>
                                                                <th>Vaga</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.UNIDADE.VEICULO))
                                                            {
                                                                <tr>
                                                                    <td>@cond.VEIC_NM_PLACA</td>
                                                                    <td>@cond.VEIC_NM_MARCA</td>
                                                                    <td>@cond.VAGA.VAGA_NM_EXIBE</td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Visitantes*@
                            <div id="tab-4" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Em Desenvolvimento</h4>
                                                <br />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Encomendas*@
                            <div id="tab-5" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Encomendas da Unidade</h4>
                                                <br />
                                                @if (Model.UNIDADE.ENCOMENDA.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Data de Chegada</th>
                                                                <th>Data de Entrega</th>
                                                                <th>Tipo</th>
                                                                <th>Forma de Entrega</th>
                                                                <th>Código</th>
                                                                <th>Descrição</th>
                                                                <th class="td-one-action">Ações</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.UNIDADE.ENCOMENDA))
                                                            {
                                                                <tr>
                                                                    <td>@cond.ENCO_DT_CHEGADA.Value.ToShortDateString()</td>
                                                                    <td>@cond.ENCO_DT_ENTREGA.Value.ToShortDateString()</td>
                                                                    <td>@cond.TIPO_ENCOMENDA.TIEN_NM_NOME</td>
                                                                    <td>@cond.FORMA_ENTREGA.FOEN_NM_NOME</td>
                                                                    <td style="color:green; font-weight:bold">@cond.ENCO_CD_CODIGO</td>
                                                                    <td>@cond.ENCO_DS_ENCOMENDA</td>
                                                                    <td class="td-one-action">
                                                                        @Html.TableLinkDetails(cond.ENCO_CD_ID, "VerEncomenda","fa-search", "Consultar")
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Reservas*@
                            <div id="tab-6" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Reservas da Unidade</h4>
                                                <br />
                                                @if (Model.UNIDADE.RESERVA.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Data</th>
                                                                <th>Finalidade</th>
                                                                <th>Ambiente</th>
                                                                <th>Nome da Reserva</th>
                                                                <th>Situação</th>
                                                                <th class="td-one-action">Ações</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.UNIDADE.RESERVA))
                                                            {
                                                                <tr>
                                                                    <td>@cond.RESE_DT_EVENTO.ToShortDateString()</td>
                                                                    <td>@cond.FINALIDADE_RESERVA.FIRE_NM_NOME</td>
                                                                    <td>@cond.AMBIENTE.AMBI_NM_AMBIENTE</td>
                                                                    <td>@cond.RESE_NM_NOME</td>
                                                                    @if (cond.RESE_IN_STATUS == 1)
                                                                    {
                                                                        <td><span class="badge badge-success">Em Aprovação</span></td>
                                                                    }
                                                                    else if (cond.RESE_IN_STATUS == 2)
                                                                    {
                                                                        <td><span class="badge badge-warning">Aprovada</span></td>
                                                                    }
                                                                    else if (cond.RESE_IN_STATUS == 3)
                                                                    {
                                                                        <td><span class="badge badge-primary">Confirmada</span></td>
                                                                    }
                                                                    else if (cond.RESE_IN_STATUS == 4)
                                                                    {
                                                                        <td><span class="badge badge-info">Encerrada</span></td>
                                                                    }
                                                                    else if (cond.RESE_IN_STATUS == 5)
                                                                    {
                                                                        <td><span class="badge badge-danger">Cancelada</span></td>
                                                                    }
                                                                    else if (cond.RESE_IN_STATUS == 6)
                                                                    {
                                                                        <td><span class="badge badge-danger">Não Aprovada</span></td>
                                                                    }
                                                                    <td class="td-one-action">
                                                                        @Html.TableLinkDetails(cond.RESE_CD_ID, "VerReserva","fa-search", "Consultar")
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Mudanças*@
                            <div id="tab-7" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Mudanças da Unidade</h4>
                                                <br />
                                                @if (Model.UNIDADE.SOLICITACAO_MUDANCA.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Data</th>
                                                                <th>Entrada/Saída</th>
                                                                <th>Situação</th>
                                                                <th class="td-one-action">Ações</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.UNIDADE.SOLICITACAO_MUDANCA))
                                                            {
                                                                <tr>
                                                                    <td>@cond.SOMU_DT_MUDANCA.ToShortDateString()</td>
                                                                    @if (cond.SOMU_IN_ENTRADA_SAIDA == 1)
                                                                    {
                                                                        <td>Entrada</td>
                                                                    }
                                                                    else
                                                                    {
                                                                        <td>Saída</td>
                                                                    }
                                                                    <td>@cond.SOMU_DT_EXECUCAO_FINAL.Value.ToShortDateString()</td>
                                                                    @if (cond.SOMU_IN_STATUS == 1)
                                                                    {
                                                                        <td><span class="badge badge-success">Em Aprovação</span></td>
                                                                    }
                                                                    else if (cond.SOMU_IN_STATUS == 2)
                                                                    {
                                                                        <td><span class="badge badge-warning">Aprovada</span></td>
                                                                    }
                                                                    else if (cond.SOMU_IN_STATUS == 3)
                                                                    {
                                                                        <td><span class="badge badge-primary">Não Aprovada</span></td>
                                                                    }
                                                                    else if (cond.SOMU_IN_STATUS == 4)
                                                                    {
                                                                        <td><span class="badge badge-info">Executada</span></td>
                                                                    }
                                                                    else if (cond.SOMU_IN_STATUS == 5)
                                                                    {
                                                                        <td><span class="badge badge-danger">Cancelada</span></td>
                                                                    }
                                                                    <td class="td-one-action">
                                                                        @Html.TableLinkDetails(cond.SOMU_CD_ID, "VerMudanca","fa-search", "Consultar")
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Autorizações*@
                            <div id="tab-8" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Autorizações / Restrições</h4>
                                                <br />
                                                @if (Model.UNIDADE.AUTORIZACAO_ACESSO.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Nome</th>
                                                                <th>Tipo</th>
                                                                <th>Início</th>
                                                                <th>Término</th>
                                                                <th class="td-one-action">Ações</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.UNIDADE.AUTORIZACAO_ACESSO))
                                                            {
                                                                <tr>
                                                                    <td>@cond.AUAC_NM_VISITANTE</td>
                                                                    @if (cond.AUAC_IN_TIPO == 1)
                                                                    {
                                                                        <td>Autorização</td>
                                                                    }
                                                                    else
                                                                    {
                                                                        <td>Restrição</td>
                                                                    }
                                                                    <td>@cond.AUAC_DT_INICIO.Value.ToShortDateString()</td>
                                                                    <td>@cond.AUAC_DT_LIMITE.Value.ToShortDateString()</td>
                                                                    <td class="td-one-action">
                                                                        @Html.TableLinkDetails(cond.AUAC_CD_ID, "VerAutorizacao","fa-search", "Consultar")
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            @*Ocorrências*@
                            <div id="tab-9" class="tab-pane">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="form-group" style=" margin-left: -15px; margin-right: -15px;">
                                            <div class="col-lg-12 col-md-12 col-xs-12">
                                                <h4>Ocorrências da Unidade</h4>
                                                <br />
                                                @if (Model.UNIDADE.OCORRENCIA.Count > 0)
                                                {
                                                    <table class="table table-striped table-bordered table-hover dataTables-example">
                                                        <thead style="background-color:lightsteelblue">
                                                            <tr>
                                                                <th>Data</th>
                                                                <th>Título</th>
                                                                <th>Situação</th>
                                                                <th class="td-one-action">Ações</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (var cond in (Model.UNIDADE.OCORRENCIA))
                                                            {
                                                                <tr>
                                                                    <td>@cond.OCOR_DT_OCORRENCIA.ToShortDateString()</td>
                                                                    <td>@cond.OCOR_NM_TITULO</td>
                                                                    @if (cond.OCOR_IN_STATUS == 1)
                                                                    {
                                                                        <td><span class="badge badge-success">Ativa</span></td>
                                                                    }
                                                                    else
                                                                    {
                                                                        <td><span class="badge badge-primary">Encerrada</span></td>
                                                                    }
                                                                    <td class="td-one-action">
                                                                        @Html.TableLinkDetails(cond.OCOR_CD_ID, "VerOcorrencia","fa-search", "Consultar")
                                                                    </td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <br />
                    @Html.ButtonLinkIndex(text: "Voltar", action: "Voltar", controller: "Morador")
                </div>
            </div>
        </div>
    </div>
</div>


@section Styles {
    <link href="~/Content/themes/base/all.css" rel="stylesheet" />
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/plugins/jasnyBootstrapStyles")
    @Styles.Render("~/plugins/toastrStyles")
}
@section Scripts {
    @Scripts.Render("~/plugins/toastr")
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/plugins/jasnyBootstrap")
    @Scripts.Render("~/plugins/sparkline")
